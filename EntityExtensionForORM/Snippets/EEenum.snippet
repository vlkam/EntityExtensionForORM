<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Entity Extension for enumeration</Title>
      <Author>vlkam</Author>
      <Description></Description>
      <HelpUrl></HelpUrl>
      <Shortcut>eeen</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>Type</ID>
          <ToolTip>Type</ToolTip>
          <Default>string</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>Name</ID>
          <ToolTip>Name</ToolTip>
          <Default>Name</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="csharp">
          <![CDATA[public $Type$ $Name$ { get { return GetEnumeration<$Type$>(ref $Name$_); } set { SetEnumeration(ref $Name$_, value); } } int? $Name$_;]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>